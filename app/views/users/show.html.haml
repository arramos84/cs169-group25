#wrap
  = render("static_pages/navigation.html.haml")
  %div.container-fluid
    - flash.each do |key,value|
      %div{:class => "alert alert-#{key}"}= value  
    %div.marketing.row-fluid
      %div.span12
      %h2{:style=>'text-align: center'}
        Welcome #{@user.first_name.titleize}! Your personality type is: #{@user.survey.personality_type}
      %h3{:style=>'text-align: center'} Let's learn more about your personality!
      %br
      %div.row-fluid
        %div.span3.offset1(class="chart_survey")
          %div(id="chart")
          = render_chart(@survey_chart, 'chart')
        %div.span5
          %br
          %p{'text-indent'=>'100px'}
            #{@body}
      %div.row-fluid
        %div.span6.offset3
          %br
          %iframe{:width => "560", :height => "315",:frameborder => "0", :src =>"http://www.youtube.com/embed/#{@video_url}",:allowFullScreen => ""}

      %br


          
#push

= render "static_pages/footer.html.haml"